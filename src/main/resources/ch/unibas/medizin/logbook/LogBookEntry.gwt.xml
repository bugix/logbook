<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<module rename-to="logbook">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />
	<inherits name="com.google.gwt.user.theme.standard.Standard" />
	<inherits name="com.google.web.bindery.requestfactory.RequestFactory" />
	<!-- <inherits name="com.google.gwt.user.cellview.CellView"/> -->
	<inherits name="com.google.gwt.logging.Logging" />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name="com.google.gwt.text.Text" />
	<inherits name="ch.unibas.medizin.logbook.client.style.LogBookStyle" />
	<!-- For development a default of `DEBUG` is recommended -->
	<inherits name="com.allen_sauer.gwt.log.gwt-log-INFO" />
	<set-property name="log_DivLogger" value="DISABLED" />
	<set-property name="log_WindowLogger" value="DISABLED" />
	<inherits name="com.google.gwt.http.HTTP" />

	<inherits name="de.novanic.eventservice.GWTEventService" />

	<source path="client" />
	<source path="shared" />
	<public path="public" />

	<!-- Logging Configuration -->
	<set-property name="gwt.logging.enabled" value="TRUE" />
	<set-property name="gwt.logging.logLevel" value="INFO" />
	<set-property name="gwt.logging.consoleHandler" value="DISABLED" />
	<set-property name="gwt.logging.developmentModeHandler" value="ENABLED" />
	<set-property name="gwt.logging.firebugHandler" value="ENABLED" />
	<set-property name="gwt.logging.hasWidgetsHandler" value="DISABLED" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
	<set-property name="gwt.logging.systemHandler" value="ENABLED" />
	<set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />

	<!--UiBinder Configuration -->
	<set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" />

	<entry-point class="ch.unibas.medizin.logbook.client.application.ApplicationEntry" />
	
	<set-property name="user.agent" value="safari"/>
</module>
