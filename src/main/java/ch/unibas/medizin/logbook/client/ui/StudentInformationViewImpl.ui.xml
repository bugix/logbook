<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:cell="urn:import:com.google.gwt.cell.client" xmlns:custom="urn:import:ch.unibas.medizin.logbook.client.widget">
	<ui:style src="styles.css">
	.mainVerticlePanelStyle {
		width: 100%;
		height: 100%;
	}
	
	.informationHPStyle {
		height: 30%;
		width: 100%;
		padding-bottom: 10px;
		padding-left: 2px;
		vertical-align: inherit;
	}
	
	.personnelInforamationSPStyle {
		border-style: solid;
		border-width: 1px;
		border-radius: 5px;
		border-color: lightblue;
		padding: 15px;
		width: 91%;
		height: 90%;
	}
	
	.currentProgressSPStyle {
		border-style: solid;
		border-width: 1px;
		border-radius: 5px;
		border-color: lightblue;
		padding: 15px 15px 15px 5px;
		width: 92%;
		padding-left: 15px;
		padding-top: 15px;
	}
	
	.latestSkillHPStyle {
		width: 100%;
		height: 70%;
		border-width: 1px;
		border-style: solid;
		spacing: 0;
	}
	
	.infoLabelStyle {
		font-style: normal;
		font-size: 98%;
		color: #888888;
		display: block;
		padding-top: 5px;
		float: left;
	}
	
	.progressInfoLabelStyle {
		font-style: normal;
		font-size: 98%;
		color: #888888;
		display: block;
		padding-top: 5px;
		float: left;
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipsis;
		width: 230px;
	}
	
	.valueLabelStyle {
		font-size: 97%;
		font-style: normal;
		color: #333333;
		display: block;
		float: left;
		padding-top: 5px;
		padding-left: 10px;
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipsis;
		width: 200px;
	}
	
	.headingLabelStyle {
		font-style: normal;
		color: #888888;
		font-size: 124%;
		line-height: 32px;
		padding-bottom: 1px;
		text-transform: uppercase;
	}
	
	.skillAcquiredheadingLabelStyle {
		font-style: normal;
		color: #888888;
		font-size: 124%;
		line-height: 32px;
		padding-bottom: 1px;
		padding-left: 18px;
		text-transform: uppercase;
	}
	
	.finalizeButtonStyle {
		border-radius: 5px 5px 5px 5px;
		padding: 7px 14px 2px;
		border: 1px solid #77D5F7;
		height: 22px;
	}
	
	.tableBodyStyle {
		padding-right: 5px;
	}
	
	.progressBarLeftPadding {
		
	}
	
	.latestAcquiredSkillSPStyle {
		border-style: solid;
		border-width: 1px;
		border-radius: 5px;
		border-color: lightblue #77D5F7;
		padding-top: 12px;
		width: 100%;
	}
	
	.skillTableSize {
		width: 100%;
		padding: 15px;
	}
	
	.trStyle {
		height: 20px;
		float: left;
	}
	
	.tablePadding {
		padding-bottom: 29px;
	}
	
	.mainTrStyle {
		height: 30%;
	}
	
	.subMainTrStyle {
		height: 70%;
		padding-top: 10px;
	}
	
	.tabPanelStyle {
		border-radius: 5px;
	}
	
	.changeBtnTrStyle {
		float: right;
	}
	
	.latestAcquiredSkillScrollPanelStyle {
		height: 100%;
		overflow: scroll;
		width: 100%;
	}
	
	.skillTablePagerStyle {
		float: right;
	}
	
	.mainErrorLabelStyle {
		width: 94%;
		height: 40%;
		background: url("logbook/gwt/logbook/images/ui-bg_glass_55_f8da4e_1x400.png") repeat-x scroll 50% 50% #F8DA4E;
		background-repeat: repeat-x;
		color: #A25617;
		border-style: solid;
		border-width: 1px;
		border-radius: 5px;
		border-color: #FCD113;
		font-family: Verdana, Arial, sans-serif;
		padding-top: 5px;
		padding-bottom: 5px;
		padding-left: 47px;
		margin-bottom: 10px;
	}
	</ui:style>
	<g:FocusPanel ui:field="studentFocusPanel">
		<g:HTMLPanel>
			<g:VerticalPanel addStyleNames="{style.mainVerticlePanelStyle}"
				verticalAlignment="top">
				<g:HTMLPanel>
					<table>
						<tbody>
							<tr>
								<g:HTMLPanel ui:field="hpErrorMessage">
									<div class="ui-state-highlight ui-corner-all mainErrorLabelStyle"
										style="padding:5px; margin-bottom:10px;">
										<div class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></div>
										<div ui:field="lblErrorMessage"></div>
									</div>
								</g:HTMLPanel>
							</tr>
							<tr addStyleNames="{style.mainTrStyle}">
								<g:HorizontalPanel addStyleNames="{style.informationHPStyle}">
									<g:HTMLPanel addStyleNames="{style.personnelInforamationSPStyle}">
										<g:Label ui:field="lblPersonnelInformation"
											addStyleNames="{style.headingLabelStyle}"></g:Label>
										<table>
											<tbody>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblName" addStyleNames="{style.infoLabelStyle}"></g:Label>
													</td>
													<td>
														<g:Label ui:field="lblNameVal" addStyleNames="{style.valueLabelStyle}"></g:Label>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblStudentId" addStyleNames="{style.infoLabelStyle}"></g:Label>
													</td>
													<td>
														<g:Label ui:field="lblStudentIdVal" addStyleNames="{style.valueLabelStyle}"></g:Label>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblStudyYear" addStyleNames="{style.infoLabelStyle}">
														</g:Label>
													</td>
													<td>
														<g:Label ui:field="lblStudeyYearvalue"
															addStyleNames="{style.valueLabelStyle}"></g:Label>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblEmail" addStyleNames="{style.infoLabelStyle}">
														</g:Label>
													</td>
													<td>
														<g:Label ui:field="lblEmailVal" addStyleNames="{style.valueLabelStyle}"></g:Label>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
													</td>
													<td>
														<g:Anchor ui:field="btnChange" addStyleNames="{style.changeBtnTrStyle}"></g:Anchor>
													</td>
												</tr>

											</tbody>
										</table>
									</g:HTMLPanel>

									<g:HTMLPanel
										addStyleNames="{style.currentProgressPanelStyle} {style.personnelInforamationSPStyle}">
										<table>
											<tbody>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblCurrentProgress"
															addStyleNames="{style.headingLabelStyle}"></g:Label>
													</td>
												</tr>

												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblLevel1Progress"
															addStyleNames="{style.progressInfoLabelStyle}"></g:Label>
													</td>
													<td>
														<custom:CustomProgressbar
															ui:field="prgBarLevel1" addStyleNames="{style.progressBarLeftPadding}"></custom:CustomProgressbar>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblLevel2Progress"
															addStyleNames="{style.progressInfoLabelStyle}"></g:Label>
													</td>
													<td>
														<custom:CustomProgressbar
															ui:field="prgBarLevel2" addStyleNames="{style.progressBarLeftPadding}"></custom:CustomProgressbar>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label ui:field="lblTotalProgress"
															addStyleNames="{style.progressInfoLabelStyle}"></g:Label>
													</td>
													<td>
														<custom:CustomProgressbar
															ui:field="prgBarTotal" addStyleNames="{style.progressBarLeftPadding}"></custom:CustomProgressbar>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
													</td>
													<td>
													</td>
												</tr>
												<tr addStyleNames="{style.trStyle}">
													<td>
														<g:Label addStyleNames="{style.infoLabelStyle}">&nbsp;
														</g:Label>
													</td>
													<td>
														<g:ToggleButton ui:field="btnFinalizeLogBook"
															addStyleNames="{style.finalizeButtonStyle}"></g:ToggleButton>
													</td>
												</tr>
											</tbody>
										</table>
									</g:HTMLPanel>
								</g:HorizontalPanel>
							</tr>
							<tr addStyleNames="{style.subMainTrStyle}">
								<g:VerticalPanel height="100%" width="100%"
									addStyleNames="{style.latestAcquiredSkillSPStyle}">
									<g:Label ui:field="lblLatestAcquiredSkill" addStyleNames="{style.skillAcquiredheadingLabelStyle}"></g:Label>
									<g:ScrollPanel addStyleNames="{style.latestAcquiredSkillScrollPanelStyle}">
										<g:VerticalPanel width="100%" height="100%">
											<c:CellTable ui:field="table" addStyleNames="{style.skillTableSize}" />
											<c:SimplePager ui:field="pager" display="{table}" addStyleNames="{style.skillTablePagerStyle}" />
										</g:VerticalPanel>
									</g:ScrollPanel>
								</g:VerticalPanel>
							</tr>
							<tr>
								<custom:CustomCopyrightWidget></custom:CustomCopyrightWidget>
							</tr>
						</tbody>
					</table>
				</g:HTMLPanel>
			</g:VerticalPanel>
		</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder> 
