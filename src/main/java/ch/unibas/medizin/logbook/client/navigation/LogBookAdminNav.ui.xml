<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.textBoxStyle {
		color: #888888;
		margin: 0;
		width: 180px;
		height: 30px;
		margin: 7px;
	}
	
	.buttonStyle {
		margin: 0;
		width: 185px;
		margin: 7px;
		height: 34px;
		font-size: medium;
	}
	
	.buttonStyle:hover {
		color: #1A68A0;
	}
	
	.mainVerticlePanelStyle {
		height: auto;
		border: 1px solid lightblue !important;
		border-radius: 4px;
		padding: 20px;
	}
	
	.studentLoginPanelStyle {
		border: 1px solid lightblue !important;
		border-radius: 4px;
		height: 60px;
		text-indent: 10px;
		color: #222222;
		width: 100%;
		text-align: left;
		margin-top: 30px;
	}
	
	.linkStyle {
		color: #222222;
		margin-left: 8px;
		font-size: 11px;
	}
	
	.linkStyle:hover {
		color: #222222;
		margin-left: 8px;
		text-decoration: underline;
		font-size: 11px;
	}
	</ui:style>
	<g:TabLayoutPanel ui:field="mainLogBookTabpanel" barUnit='PX' barHeight='51' height="100%">
		<g:tab>
			<g:customHeader>
				<g:Label ui:field="login"></g:Label>
			</g:customHeader>
			<g:SimplePanel>
			</g:SimplePanel>
		</g:tab>
	</g:TabLayoutPanel>

</ui:UiBinder> 
