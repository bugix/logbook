<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:logbook.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:logbook.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  
        
        
        
        
    
        
        
        
        
    <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}">
                <g:TextBox ui:field="description"/>
            </div>
        </div>
        <div id="shortcut">
            <div class="{style.label}">Shortcut</div>
            <div class="{style.value}">
                <g:TextBox ui:field="shortcut"/>
            </div>
        </div>
        <div id="topic">
            <div class="{style.label}">Topic</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="topic"/>
            </div>
        </div>
        <div id="skillLevel">
            <div class="{style.label}">Skill Level</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="skillLevel"/>
            </div>
        </div>
        <div id="skillsAcquired">
            <div class="{style.label}">Skills Acquired</div>
            <div class="{style.value}">
                <a:SkillAcquiredSetEditor ui:field="skillsAcquired"/>
            </div>
        </div>
        <div id="keywords">
            <div class="{style.label}">Keywords</div>
            <div class="{style.value}">
                <a:KeywordSetEditor ui:field="keywords"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
