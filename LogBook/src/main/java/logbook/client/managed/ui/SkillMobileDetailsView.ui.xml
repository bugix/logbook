<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div id="id">
            <div class="{style.label}">Id</div>
            <div class="{style.value}" ui:field="id"/>
        </div>
    <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}" ui:field="description"/>
        </div>
    <div id="shortcut">
            <div class="{style.label}">Shortcut</div>
            <div class="{style.value}" ui:field="shortcut"/>
        </div>
    <div id="topic">
            <div class="{style.label}">Topic</div>
            <div class="{style.value}" ui:field="topic"/>
        </div>
    <div id="skillLevel">
            <div class="{style.label}">Skill Level</div>
            <div class="{style.value}" ui:field="skillLevel"/>
        </div>
    <div id="german_text">
            <div class="{style.label}">German_text</div>
            <div class="{style.value}" ui:field="german_text"/>
        </div>
    <div id="skillsAcquired">
            <div class="{style.label}">Skills Acquired</div>
            <div class="{style.value}" ui:field="skillsAcquired"/>
        </div>
    <div id="keywords">
            <div class="{style.label}">Keywords</div>
            <div class="{style.value}" ui:field="keywords"/>
        </div>
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
