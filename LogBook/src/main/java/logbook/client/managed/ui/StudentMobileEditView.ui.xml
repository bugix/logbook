<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:logbook.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:logbook.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  <div id="studentId">
            <div class="{style.label}">Student Id</div>
            <div class="{style.value}">
                <g:TextBox ui:field="studentId"/>
            </div>
        </div>
        <div id="shib_id">
            <div class="{style.label}">Shib_id</div>
            <div class="{style.value}">
                <g:TextBox ui:field="shib_id"/>
            </div>
        </div>
        <div id="email">
            <div class="{style.label}">Email</div>
            <div class="{style.value}">
                <g:TextBox ui:field="email"/>
            </div>
        </div>
        <div id="gender">
            <div class="{style.label}">Gender</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="gender"/>
            </div>
        </div>
        <div id="name">
            <div class="{style.label}">Name</div>
            <div class="{style.value}">
                <g:TextBox ui:field="name"/>
            </div>
        </div>
        <div id="preName">
            <div class="{style.label}">Pre Name</div>
            <div class="{style.value}">
                <g:TextBox ui:field="preName"/>
            </div>
        </div>
        <div id="studentStatus">
            <div class="{style.label}">Student Status</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="studentStatus"/>
            </div>
        </div>
        <div id="studyYear">
            <div class="{style.label}">Study Year</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="studyYear"/>
            </div>
        </div>
        <div id="skillAcquired">
            <div class="{style.label}">Skill Acquired</div>
            <div class="{style.value}">
                <a:SkillAcquiredSetEditor ui:field="skillAcquired"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
